<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="exitView" prefHeight="500.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="VSBakery.ExitViewController">
   <children>
      <Label layoutX="190.0" layoutY="149.0" prefHeight="62.0" prefWidth="210.0" text="Order is been Processing!">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="217.0" layoutY="23.0" prefHeight="42.0" prefWidth="183.0" text="VS Bakery" textFill="#3509b9">
         <font>
            <Font name="Franklin Gothic Heavy Italic" size="36.0" />
         </font>
      </Label>
      <Button fx:id="newOrder" layoutX="157.0" layoutY="370.0" mnemonicParsing="false" onAction="#newOrderHandler" prefHeight="42.0" prefWidth="120.0" text="New Order">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Button>
      <Button fx:id="exit" layoutX="316.0" layoutY="370.0" mnemonicParsing="false" onAction="#exitHandler" prefHeight="42.0" prefWidth="120.0" text="Exit">
         <font>
            <Font name="Arial" size="18.0" />
         </font>
      </Button>
      <Label fx:id="message" alignment="CENTER" contentDisplay="CENTER" layoutX="-1.0" layoutY="211.0" prefHeight="62.0" prefWidth="600.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
